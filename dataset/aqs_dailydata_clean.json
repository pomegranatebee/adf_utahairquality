{
	"name": "aqs_dailydata_clean",
	"properties": {
		"linkedServiceName": {
			"referenceName": "utahaq_blob_output",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"pollutants": {
				"type": "string"
			},
			"datestart": {
				"type": "string"
			},
			"dateend": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().pollutants,'_',dataset().datestart,'_',dataset().dateend,'.csv')",
					"type": "Expression"
				},
				"folderPath": "dailydata",
				"container": "aqsclean"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}