{
	"name": "aqs_dailydata",
	"properties": {
		"activities": [
			{
				"name": "pollutants",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "pollutants",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "for_each_pollutant",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "pollutants",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('pollutants').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy raw JSON to aqsraw",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET",
									"paginationRules": {
										"supportRFC5988": "true"
									}
								},
								"sink": {
									"type": "JsonSink",
									"storeSettings": {
										"type": "AzureBlobStorageWriteSettings"
									},
									"formatSettings": {
										"type": "JsonWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "aqs_dailydata_input",
									"type": "DatasetReference",
									"parameters": {
										"pollutants": "@item().pollutant_id",
										"datestart": {
											"value": "@variables('datestart')",
											"type": "Expression"
										},
										"dateend": {
											"value": "@variables('dateend')",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "aqs_dailydata_raw",
									"type": "DatasetReference",
									"parameters": {
										"pollutants": "@item().pollutant_id",
										"datestart": {
											"value": "@variables('datestart')",
											"type": "Expression"
										},
										"dateend": {
											"value": "@variables('dateend')",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Transform JSON to CSV and copy to aqsclean",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "Copy raw JSON to aqsraw",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "JsonSource",
									"storeSettings": {
										"type": "AzureBlobStorageReadSettings",
										"recursive": true,
										"enablePartitionDiscovery": false
									},
									"formatSettings": {
										"type": "JsonReadSettings"
									}
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobStorageWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"path": "$['Data'][0]['date_local']"
											},
											"sink": {
												"name": "date_local",
												"type": "DateTime"
											}
										},
										{
											"source": {
												"path": "$['Data'][0]['arithmetic_mean']"
											},
											"sink": {
												"name": "arithmetic_mean",
												"type": "Decimal"
											}
										}
									],
									"collectionReference": "",
									"mapComplexValuesToString": false
								}
							},
							"inputs": [
								{
									"referenceName": "aqs_dailydata_raw",
									"type": "DatasetReference",
									"parameters": {
										"pollutants": "@item().pollutant_id",
										"datestart": {
											"value": "@variables('datestart')",
											"type": "Expression"
										},
										"dateend": {
											"value": "@variables('dateend')",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "aqs_dailydata_clean",
									"type": "DatasetReference",
									"parameters": {
										"pollutants": "@item().pollutant_id",
										"datestart": {
											"value": "@variables('datestart')",
											"type": "Expression"
										},
										"dateend": {
											"value": "@variables('dateend')",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"variables": {
			"datestart": {
				"type": "String",
				"defaultValue": "20211201"
			},
			"dateend": {
				"type": "String",
				"defaultValue": "20211202"
			}
		},
		"annotations": [],
		"lastPublishTime": "2022-04-09T17:39:19Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}