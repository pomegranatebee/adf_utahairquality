{
	"name": "aqs_dailydata",
	"properties": {
		"activities": [
			{
				"name": "pollutants",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "pollutants",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "for_each_pollutant",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "pollutants",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('pollutants').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy raw JSON to aqsraw",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET",
									"paginationRules": {
										"supportRFC5988": "true"
									}
								},
								"sink": {
									"type": "JsonSink",
									"storeSettings": {
										"type": "AzureBlobStorageWriteSettings"
									},
									"formatSettings": {
										"type": "JsonWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "aqs_dailydata_input",
									"type": "DatasetReference",
									"parameters": {
										"pollutants": "@item().pollutant_id",
										"datestart": {
											"value": "@variables('datestart')",
											"type": "Expression"
										},
										"dateend": {
											"value": "@variables('dateend')",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "aqs_dailydata_raw",
									"type": "DatasetReference",
									"parameters": {
										"pollutants": "@item().pollutant_id",
										"datestart": {
											"value": "@variables('datestart')",
											"type": "Expression"
										},
										"dateend": {
											"value": "@variables('dateend')",
											"type": "Expression"
										}
									}
								}
							]
						},
						{
							"name": "Transform JSON to CSV and copy to aqsclean",
							"type": "ExecuteDataFlow",
							"dependsOn": [
								{
									"activity": "Copy raw JSON to aqsraw",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "1.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataflow": {
									"referenceName": "aqsjsontocsv",
									"type": "DataFlowReference",
									"parameters": {
										"year": {
											"value": "'@{substring(string(variables('datestart')),0,4)}'",
											"type": "Expression"
										},
										"pollutant": {
											"value": "'@{item().pollutant_id}'",
											"type": "Expression"
										}
									},
									"datasetParameters": {
										"aqsjson": {
											"pollutants": "@item().pollutant_id",
											"datestart": {
												"value": "@variables('datestart')",
												"type": "Expression"
											},
											"dateend": {
												"value": "@variables('dateend')",
												"type": "Expression"
											}
										}
									}
								},
								"integrationRuntime": {
									"referenceName": "HigherTTL",
									"type": "IntegrationRuntimeReference"
								},
								"traceLevel": "None",
								"cacheSinks": {
									"firstRowOnly": true
								}
							}
						}
					]
				}
			}
		],
		"variables": {
			"datestart": {
				"type": "String",
				"defaultValue": "20060101"
			},
			"dateend": {
				"type": "String",
				"defaultValue": "20061231"
			}
		},
		"annotations": [],
		"lastPublishTime": "2022-04-09T17:39:19Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}